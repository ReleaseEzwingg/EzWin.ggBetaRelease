local function RANJ_fake_script() -- switch_2.bgp 
	local script = Instance.new('LocalScript', switch_2)

	local Players = game:GetService("Players")
	local UserInputService = game:GetService("UserInputService")
	local button = script.Parent
	
	local player = Players.LocalPlayer
	local bhopEnabled = false
	local canJump = true
	local jumpConnection
	
	-- Forward lunge function
	local function lungeForward()
		local character = player.Character or player.CharacterAdded:Wait()
		local hrp = character:WaitForChild("HumanoidRootPart")
	
		local direction = hrp.CFrame.LookVector * 100 -- Adjust speed here
		hrp.Velocity = Vector3.new(direction.X, hrp.Velocity.Y, direction.Z)
	end
	
	-- Enable infinite jump with 1 sec cooldown
	local function enableBhop()
		if bhopEnabled then return end
		bhopEnabled = true
		print("Bhop Enabled")
	
		-- Connect jump request
		jumpConnection = UserInputService.JumpRequest:Connect(function()
			if bhopEnabled and canJump then
				local character = player.Character
				if character then
					local humanoid = character:FindFirstChildOfClass("Humanoid")
					if humanoid and humanoid:GetState() ~= Enum.HumanoidStateType.Dead then
						canJump = false
						humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
						lungeForward()
	
						-- Cooldown
						task.delay(0.3, function()
							canJump = true
						end)
					end
				end
			end
		end)
	end
	
	local function disableBhop()
		if not bhopEnabled then return end
		bhopEnabled = false
		print("Bhop Disabled")
		if jumpConnection then
			jumpConnection:Disconnect()
		end
	end
	
	-- Button toggles bhop mode
	button.MouseButton1Click:Connect(function()
		if not bhopEnabled then
			enableBhop()
		else
			disableBhop()
		end
	end)
	
end
coroutine.wrap(RANJ_fake_script)()
local function YNDVH_fake_script() -- viewport.LocalScript 
	local script = Instance.new('LocalScript', viewport)

	game:GetService("RunService").Stepped:Connect(function()
		script.Parent.CurrentCamera = workspace.CurrentCamera
		for _,v in next,script.Parent:GetDescendants() do
			if v ~= script then
				v:Destroy()
			end
		end
		for _,a in next,game.Players:GetPlayers() do
			for _,m in next,a.Character:GetDescendants() do
				if m:IsA("BasePart") and m.Name ~= "HumanoidRootPart" then
					local m2 = m:Clone()
					m2.Transparency = 0.5
					m2.Parent = script.Parent
					m2.Material = Enum.Material.SmoothPlastic
					if a.Team ~= nil then
						m2.Color = a.Team.TeamColor.Color
					else
						m2.Color = Color3.new(1,1,1)
					end
				end
			end
		end
	end)
end
coroutine.wrap(YNDVH_fake_script)()
