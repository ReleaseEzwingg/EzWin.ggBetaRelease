local function CCGH_fake_script() -- ESP.TabSys 
	local script = Instance.new('LocalScript', ESP)

	-- Function to toggle visibility of entities in a tab
	local function toggleTabVisibility(tab, entities)
		tab.MouseButton1Click:Connect(function()
			for _, entity in pairs(entities) do
				if entity then
					entity.Visible = not entity.Visible
				end
			end
		end)
	end
	
	-- Tab 1
	local tab1 = script.Parent
	local tab1Entities = {
		tab1:FindFirstChild("Bhop"),
		tab1:FindFirstChild("boxes"),
		tab1:FindFirstChild("boxtog"),
		tab1:FindFirstChild("chamtog"),
		tab1:FindFirstChild("fill"),
		tab1:FindFirstChild("filltog"),
		tab1:FindFirstChild("health"),
		tab1:FindFirstChild("healtog"),
		tab1:FindFirstChild("nametog"),
		tab1:FindFirstChild("ImageLabel"),
		tab1:FindFirstChild("Name"),
		tab1:FindFirstChild("txt2"),
		tab1:FindFirstChild("txt3"),
		tab1:FindFirstChild("txt4"),
		tab1:FindFirstChild("txt5"),
		tab1:FindFirstChild("txt6"),
		tab1:FindFirstChild("txt7"),
		tab1:FindFirstChild("txt8"),
		tab1:FindFirstChild("txt9")
	}
	toggleTabVisibility(tab1, tab1Entities)
	
	-- Tab 2 (example)
	local tab2 = script.Parent.Parent:FindFirstChild("Tab2")  -- Assuming Tab2 is a sibling of Tab1
	local tab2Entities = {
		tab2:FindFirstChild("Entity1"),
		tab2:FindFirstChild("Entity2"),
		tab2:FindFirstChild("Entity3"),
		-- Add other entities for Tab2 here
	}
	toggleTabVisibility(tab2, tab2Entities)
	
	-- You can keep repeating this pattern for as many tabs as you need.
	
end
coroutine.wrap(CCGH_fake_script)()
