local function GJSAX_fake_script() -- switch.LocalScript 
	local script = Instance.new('LocalScript', switch)

	local TweenService = game:GetService("TweenService")
	local button = script.Parent
	local frame = button.Parent:FindFirstChild("switch") -- replace with your frame's name
	local movedRight = false
	
	local tweenInfo = TweenInfo.new(0.4, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut)
	
	button.MouseButton1Click:Connect(function()
		if not frame then return end
	
		local currentPos = frame.Position
		local offset = movedRight and -28 or 28
		local newPos = currentPos + UDim2.new(0, offset, 0, 0)
	
		local tween = TweenService:Create(frame, tweenInfo, { Position = newPos })
		tween:Play()
	
		movedRight = not movedRight
	end)
	
	local bot = button:FindFirstChild("bot")
	
	
	local bot = script.Parent:WaitForChild("bot")
	
	button.MouseButton1Click:Connect(function()
		bot.Enabled =not bot.Enabled
	
	end)
	
end
coroutine.wrap(GJSAX_fake_script)()
