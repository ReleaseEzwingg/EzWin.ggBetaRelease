local function DDCR_fake_script() -- boxtog.ESP 
	local script = Instance.new('LocalScript', boxtog)

	-- References to switches
	local toggleboxes = script.Parent:FindFirstChild("switch")
	local togglefill = script.Parent.Parent.filltog:FindFirstChild("switch")
	local toggleheal = script.Parent.Parent.healtog:FindFirstChild("switch")
	local togglename = script.Parent.Parent.nametog:FindFirstChild("switch")
	
	-- ESP template
	local ESP_TEMPLATE = script:WaitForChild("ESP_BOX")
	
	-- Toggle states
	local boxEnabled = false
	local fillEnabled = false
	local healthEnabled = false
	local nameEnabled = false
	
	-- Toggle event handlers
	toggleboxes.MouseButton1Click:Connect(function()
		boxEnabled = not boxEnabled
	end)
	
	togglefill.MouseButton1Click:Connect(function()
		fillEnabled = not fillEnabled
	end)
	
	toggleheal.MouseButton1Click:Connect(function()
		healthEnabled = not healthEnabled
	end)
	
	togglename.MouseButton1Click:Connect(function()
		nameEnabled = not nameEnabled
	end)
	
	-- ESP update loop
	local RunService = game:GetService("RunService")
	RunService.RenderStepped:Connect(function()
		local Players = game.Players:GetPlayers()
		local localPlayer = game.Players.LocalPlayer
	
		for _, Plr in pairs(Players) do
			if Plr ~= localPlayer and Plr.Character and Plr.Character:FindFirstChild("HumanoidRootPart") and Plr.Character:FindFirstChild("Humanoid") then
				local Character = Plr.Character
				local HRP = Character.HumanoidRootPart
				local Humanoid = Character.Humanoid
	
				-- Clone ESP if not present
				if not HRP:FindFirstChild("ESP_BOX") then
					local NewESP = ESP_TEMPLATE:Clone()
					NewESP.Enabled = true
					NewESP.Parent = HRP
				end
	
				-- Update existing ESP
				local ESP = HRP:FindFirstChild("ESP_BOX")
				local Holder = ESP:FindFirstChild("Holder")
				if Holder then
					local Box = Holder:FindFirstChild("Box")
					local Fill = Box and Box:FindFirstChild("UIGradient")
					local Stroke = Box and Box:FindFirstChild("UIStroke")
					local HP = Holder:FindFirstChild("HP")
					local HPBar = HP and HP:FindFirstChild("Bar")
					local Name = Holder:FindFirstChild("Username")
					local Distance = Holder:FindFirstChild("Distance")
	
					-- Apply toggle states
					if Stroke then Stroke.Enabled = boxEnabled end
					if Fill then Fill.Enabled = fillEnabled end
					if HP then HP.Visible = healthEnabled end
					if Name then Name.Visible = nameEnabled end
					if Distance then Distance.Visible = nameEnabled end
	
					-- Update dynamic elements
					if HPBar then
						local healthPercent = math.clamp(Humanoid.Health / Humanoid.MaxHealth, 0, 1)
						HPBar:TweenSizeAndPosition(UDim2.new(0.976, 1, healthPercent, 1), UDim2.new(0, 0, 0, 0), "Out", "Quad", 0.2, true)
					end
	
					if Name then Name.Text = Plr.Name end
					if Distance and localPlayer.Character and localPlayer.Character:FindFirstChild("HumanoidRootPart") then
						local dist = (localPlayer.Character.HumanoidRootPart.Position - HRP.Position).Magnitude
						Distance.Text = math.floor(dist + 0.5) .. " M"
					end
				end
			end
		end
	end)
	
end
coroutine.wrap(DDCR_fake_script)()
