-- Scripts:

local function ZLHB_fake_script() -- Home.Hov 
	local script = Instance.new('LocalScript', Home)

	local TweenService = game:GetService("TweenService")
	
	local hover1 = script.Parent -- adjust as needed
	
	-- Define the tween info
	local tweenInfo = TweenInfo.new(
		0.3, -- Time (seconds)
		Enum.EasingStyle.Quad, -- Easing style
		Enum.EasingDirection.Out -- Easing direction
	)
	
	-- Define target colors
	local colorEnter = Color3.fromRGB(170, 0, 255)
	local colorLeave = Color3.fromRGB(103, 0, 103) -- equivalent to 0.403922, 0, 0.403922
	
	-- Create tweens
	hover1.MouseEnter:Connect(function()
		local tween = TweenService:Create(hover1, tweenInfo, {ImageColor3 = colorEnter})
		tween:Play()
	end)
	
	hover1.MouseLeave:Connect(function()
		local tween = TweenService:Create(hover1, tweenInfo, {ImageColor3 = colorLeave})
		tween:Play()
	end)
	
end
coroutine.wrap(ZLHB_fake_script)()
